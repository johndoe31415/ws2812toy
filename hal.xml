<?xml version="1.0" ?>
<HAL>
	<templates>
		<template name="diginputswitch">
			<pin type="input" initialpullup="active" />
			<activelow />
		</template>

		<template name="digoutput">
			<pin type="output" initialvalue="inactive" />
		</template>
		
		<template name="digoutputinv">
			<pin type="output" initialvalue="inactive" />
			<activelow />
		</template>

		<template name="digio">
			<pin type="io" initialstate="input" initialpullup="inactive" />
		</template>
	</templates>

	<portpin name="PB0" template="digoutput">
		<description value="LEDOutput" />
	</portpin>

	<portpin name="PC4" template="digoutput">
		<description value="PowerLED" />
	</portpin>

	<portpin name="PC3" template="diginputswitch">
		<description value="Switch" />
	</portpin>
</HAL>
